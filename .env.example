# Example .env file for welcomepage-api

# Supabase Service Role Key (for backend/server use only)
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key-here
SUPABASE_URL=your-supabase-url-here

# Database connection string
DATABASE_URL=sqlite:///./test.db

# Spotify API credentials (required for /api/spotify/resolve)
# Create an app at https://developer.spotify.com/dashboard and copy values here
SPOTIFY_CLIENT_ID=your-spotify-client-id
SPOTIFY_CLIENT_SECRET=your-spotify-client-secret

# Google Maps API key (required for /api/google/places/search)
# Create a key in Google Cloud Console, enable Places API. Recommended: restrict by IPs (server) and scope.
GOOGLE_MAPS_API_KEY=your-google-maps-server-key
